all: funcFit

.PHONY: funcFit
funcFit: 
	cd ${ROOT_PATH}/widgets/funcFit; ${MAKE} ROOT_PATH=${ROOT_PATH} REMOTE_ENABLED=${REMOTE_ENABLED} GDB_PORT=${GDB_PORT} OS=${OS} SIGHT_CFLAGS="${SIGHT_CFLAGS}" SIGHT_LINKFLAGS="${SIGHT_LINKFLAGS}" CC=${CC} CCC=${CCC} GSL_PATH=${REPO_PATH}/gsl


clean:
	cd ${ROOT_PATH}/widgets/funcFit; ${MAKE} clean


